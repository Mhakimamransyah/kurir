{
    "name" : "kawankoding/laravel-fcm",
    "description" : "A package to send Firebase notification cross Laravel Application",
    "authors": [
        {
            "name": "Muhammad Amirul Ihsan",
            "email": "muh.amirul.ihsan@gmail.com"
        }
    ],
    "require": {
        "php" : "^5.6|^7.0",
        "illuminate/support": "^5.1|^6.0"
    },
    "autoload" : {
        "psr-4" : {
            "Kawankoding\\Fcm\\" : "src/"
        },
        "files" : [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kawankoding\\Fcm\\FcmServiceProvider"
            ],
            "aliases" : {
                "Fcm": "Kawankoding\\Fcm\\FcmFacade"
            }
        }
    }
}
